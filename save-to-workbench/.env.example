# GCS configuration for Method 3 (file upload with manifest)
GCS_BUCKET_NAME=your-gcp-project-id-save-to-workbench
GCS_BUCKET_LOCATION=US-CENTRAL1
GCS_SERVICE_ACCOUNT_NAME=gcs-uploader-and-signer
GCS_SIGNED_URL_EXPIRATION_HOURS=1

# GitHub configuration for hosting manifest files (Method 3 only)
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_REPO_OWNER=your_github_username
GITHUB_REPO_NAME=your_repo_name
GITHUB_REPO_BRANCH=main
GITHUB_MANIFEST_CLEANUP_DELAY_SECONDS=30  # Seconds to wait before deleting manifest from GitHub
